@{
    ViewData["Title"] = "Pie";
}
<script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
<h1>Charts</h1>

<div id="line">
    <script type="text/javascript">

        window.onload = function () {
            var chart = new CanvasJS.Chart("line", {
                theme: "light2",
                animationEnabled: true,
                title: {
                    text: "Simple line Chart in ASP.NET MVC"
                },
                subtitles: [
                    { text: "line" }
                ],
                data: [
                    {
                        type: "line", //change type to bar, line, area, pie, etc
                        dataPoints: [
                            { x: 10, y: 71 },
                            { x: 20, y: 55 },
                            { x: 30, y: 50 },
                            { x: 40, y: 65 },
                            { x: 50, y: 95 },
                            { x: 60, y: 68 },
                            { x: 70, y: 28 },
                            { x: 80, y: 34 },
                            { x: 90, y: 14 }
                        ]
                    }
                ]
            });
            chart.render();
        };
    </script>
</div>