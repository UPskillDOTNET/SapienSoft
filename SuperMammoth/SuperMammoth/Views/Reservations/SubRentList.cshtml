            <th>
                @Html.DisplayNameFor(model => model.QrCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AvailableToRent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RentValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
